# -------------------------------------------------------------------
# Calculate mean values of numeric predictors to hold them constant
med_mean <- mean(data$medical_experience, na.rm = TRUE)
leg_mean <- mean(data$legal_experience, na.rm = TRUE)
con_mean <- mean(data$conlang_experience, na.rm = TRUE)
nat_mean <- mean(data$native_english, na.rm = TRUE)
# Create a small "new data" frame with combinations of the factors
pred_df <- expand.grid(
group = c("control", "intervention"),  # adjust to match your factor levels
timepoint = c("intermediate", "after"),        # adjust if your factor levels differ
medical_experience  = med_mean,
