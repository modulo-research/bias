) %>%
filter(
!is.na(medical_experience),
!is.na(legal_experience),
!is.na(conlang_experience),
!is.na(native_english)
)
# Exclude rows where timepoint == 'before'
data <- data %>% filter(timepoint != 'before')
data$timepoint <- factor(data$timepoint, levels = c("intermediate", "after"))
levels(data$timepoint)
# Run the mixed-effects model using lmerTest to get p-values
model <- lmer(
logit ~ group * timepoint + medical_experience + legal_experience +
conlang_experience + native_english +
(1 | participant_code) + (1 | topic/item_id),
data = data
)
# Display the model summary with p-values
summary(model)
##########
